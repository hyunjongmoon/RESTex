{
  "name": "etransfer",
  "version": "2.0.0",
  "groupId": "ca.interac",
  "artifactId": "etransfer",
  "rootPackage": "ca.interac.etransfer",
  "handlerPackage":"ca.interac.etransfer.handler",
  "modelPackage":"ca.interac.etransfer.model",
  "overwriteHandler": true,
  "overwriteHandlerTest": true,
  "overwriteModel": true,
  "httpPort": 8080,
  "enableHttp": false,
  "httpsPort": 8443,
  "enableHttps": true,
  "enableHttp2": true,
  "enableRegistry": false,
  "supportDb": true,
  "dbInfo": {
    "name": "mysql",
    "driverClassName": "com.mysql.jdbc.Driver",
    "jdbcUrl": "jdbc:mysql://mysql:3306/etransfer?useSSL=false",
    "username": "mysqluser",
    "password": "mysqlpw"
  },
  "supportH2ForTest": true,
  "supportClient": true,
  "dockerOrganization": "networknt"
}
