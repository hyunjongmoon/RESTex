{
  "name": "tokenization",
  "version": "1.0.0",
  "groupId": "net.lightapi",
  "artifactId": "tokenization",
  "rootPackage": "net.lightapi.tokenization",
  "handlerPackage":"net.lightapi.tokenization.handler",
  "modelPackage":"net.lightapi.tokenization.model",
  "overwriteHandler": true,
  "overwriteHandlerTest": true,
  "overwriteModel": true,
  "httpPort": 8080,
  "enableHttp": false,
  "httpsPort": 8443,
  "enableHttps": true,
  "enableHttp2": true,
  "enableRegistry": false,
  "supportDb": true,
  "dbInfo": {
    "name": "mysql",
    "driverClassName": "com.mysql.jdbc.Driver",
    "jdbcUrl": "jdbc:mysql://mysql:3306/tokenization?useSSL=false",
    "username": "mysqluser",
    "password": "mysqlpw"
  },
  "supportH2ForTest": true,
  "supportClient": false,
  "dockerOrganization": "networknt"
}
